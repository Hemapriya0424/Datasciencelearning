getwd()
install.packages("arulesViz")
install.packages("plyr")
setwd("C:/Users/hema.priya.PPTS/PycharmProjects/Lead/.idea/appr")
library(arules)
library(arulesViz)
tr <- read.transactions("skills_Java.csv", format = 'basket', sep=',')
summary(tr)
windows()
itemFrequencyPlot(tr, topN=20, type='absolute')
rules <- apriori(tr, parameter = list(supp=0.001, conf=0.8))
rules <- sort(rules, by='confidence', decreasing = TRUE)
summary(rules)
rule=inspect(rules[1:30])
write(rules, file = "sql.csv", sep = ",")
topRules <- rules[1:30]
plot(topRules, method="graph")
